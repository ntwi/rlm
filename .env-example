# Put your AI provider API keys here. Rename to `.env`
# OpenAI (for gpt-* and default models)
OPENAI_API_KEY=

# Anthropic Claude (for claude-*-sonnet, incl. claude-sonnet-4.5)
ANTHROPIC_API_KEY=

# Google Gemini (for gemini-2.5-pro and others)
GOOGLE_API_KEY=

# xAI Grok (OpenAI-compatible)
XAI_API_KEY=
# Optional: override base URL (defaults to https://api.x.ai/v1)
XAI_BASE_URL=

# Ollama (local, no API key)
# Defaults to http://localhost:11434 if unset
OLLAMA_HOST=